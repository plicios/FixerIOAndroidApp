<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/exercises_graph"
    app:startDestination="@id/rates_list_dest">
    <fragment
        android:id="@+id/rates_list_dest"
        android:name="pl.pgorny.fixerioapidisplay.ui.RowsListFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_rows_list" >
        <action
            android:id="@+id/action_rates_list_dest_to_rate_fragment_dest"
            app:destination="@id/rate_fragment_dest" >
            <argument
                android:name="rate"
                app:argType="pl.pgorny.fixerioapidisplay.data.model.RateRow" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/rate_fragment_dest"
        android:name="pl.pgorny.fixerioapidisplay.ui.RateFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_rate" >
        <argument
            android:name="rate"
            app:argType="pl.pgorny.fixerioapidisplay.data.model.RateRow" />
    </fragment>
</navigation>